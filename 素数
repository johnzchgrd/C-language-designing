#include<stdio.h>
int sum=0,cnt=0;
//判断是否为素数 
int prime(int m){
	int i;
	for(i=2;i<=m/2;i++)
	  if(m%i==0)
	    break;
	if(i>m/2&&m!=1)
	   return 1;
	return 0; 
}
//寻找素数
void seekprimes(int m,int n){
 int i;
	if(m>n){
		i=m;
		m=n;
		n=i;
	}
	for(i=m;i<=n;i++){
		if(prime(i)){
			  printf("%d\n",i);
			  cnt++;
			  sum+=i;
		}
		
	}
} 
main(){
	int n,m;
	scanf("%d%d",&n,&m);
    seekprimes(m,n);
	printf("共有%d个素数,和为%d",cnt,sum); 
	return 0; 
}
